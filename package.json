    "mongoose": "^8.18.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "i": "^0.3.7",
    "nodemon": "^3.1.10",
    "sass": "^1.92.1",
    "tailwindcss": "^4.1.13"
